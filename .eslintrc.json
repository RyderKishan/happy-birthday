{
  "env": {
    "browser": true,
    "es2021": true
  },
  "globals": {
    "location": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "prettier"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-console": "off",
    "camelcase": "warn",
    "react/no-array-index-key": "off",
    "react/jsx-filename-extension": [0, { "allow": "as-needed" }],
    "no-unused-vars": "warn"
  }
}
